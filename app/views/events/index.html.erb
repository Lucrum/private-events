<h1>All Events</h1>

<ul>
  <% @events.each do |event| %>
    <li>
      <%= event.event_type %> - <%= event.location %> - <%= event.time %>
    </li>
  <% end %>
</ul>


<% if user_signed_in? %>
  <%= link_to "Create new event", new_event_path %>
  <%= button_to "Sign Out", destroy_user_session_path, method: :delete %>
<% else %>
  <%= button_to "Sign In", new_user_session_path %>
<% end %>